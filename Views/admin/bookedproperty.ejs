<%- include("./header.ejs") %> <%- include("./sidebar.ejs") %>
<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <div class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1 class="m-0">Display</h1>
        </div>
        <!-- /.col -->
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="#">Home</a></li>
            <li class="breadcrumb-item active">Display</li>
          </ol>
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
  </div>
  <!-- /.content-header -->
  <!-- </div> -->
  <div class="card">
    <!-- /.card-header -->
    <div class="card-body">
      <table id="example1" class="table table-bordered table-striped">
        <thead>
          <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Property Name</th>
            <th>Address</th>
            <th>Phone NO.</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% bp.forEach((item)=>{ %>
            <tr>
              <td><%= item.id %></td>
              <td><%= item.name %></td>
              <td><%= item.email %></td>
              <td><%= item.propertyName %></td>
              <td><%= item.address %></td>
              <td><%= item.phone %></td>
              <td>
                <a
                href="/admin/Bookproperty/delete/<%= item.id %>"
                class="btn btn-info"
                ><i class="fa-solid fa-trash"></i
              ></a>
              </td>
            </tr>
          <%  }) %>
        </tbody>
      </table>
    </div>
    <!-- /.card-body -->
  </div>
  <!-- /.card -->
</div>

<!-- /.modal -->

<%- include("./footer.ejs") %>
